    "gatsby-theme-datocms": "*",
    "gatsby-theme-ui": "*",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "eslint-config-google": "^0.13.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.1"
  }
}
