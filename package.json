{
  "name": "gatsby-website-demo",
  "version": "0.0.1",
  "description": "Simple gatsby website demo",
  "main": "gatsby-config.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "format": "prettier --write src/**/*.{js,jsx,scss,html}",
    "clean": "find . -type f -name '._*' -delete"
  },
  "dependencies": {
